{
	"auth": {
		"cognitoapplicants": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation"
		}
	},
	"api": {
		"application": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"securityType": "AMAZON_COGNITO_USER_POOLS"
			}
		},
		"startapplication": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "startapplication",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"function": {
		"startapplication": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": []
		}
	}
}